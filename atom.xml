   <id>http://liangqiu.net{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
   <author>
     <name>Liang Qiu</name>
     <uri>https://liangqiu.net/about.html</uri>
   </author>
 </entry>
 {% endfor %}
</feed>
