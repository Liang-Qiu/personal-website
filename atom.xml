<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>Liang Qiu</title>
 <link href="https://liangqiu.net/atom.xml" rel="self"/>
 <link href="https://liangqiu.net/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://liangqiu.net/</id>
